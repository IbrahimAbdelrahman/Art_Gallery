<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>Dutch Treat: @ViewBag.Title</title>
    <link href="~/css/site.css" rel="stylesheet" />
</head>
<body>
    <header>
        <h1> Welcome to our Art Gallery</h1>
        <menu>
            <ul>
                <li> <a  asp-action="Index">Home</a> </li>
                <li> <a  asp-action="Contact">Contact</a> </li>
                <li> <a  asp-action="About">About</a> </li>
            </ul>
        </menu>
    </header>
    <section>
        <h2>@ViewBag.Title</h2>
        @RenderBody()
    </section>
    <footer>Copyright 2020 Art Gallery   </footer>

    <script src="~/node_modules/jquery/dist/jquery.min.js"></script>
    <script src="~/js/index.js"></script>
</body>
</html>